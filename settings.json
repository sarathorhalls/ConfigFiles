    "workbench.iconTheme": "vscode-icons",
    "redhat.telemetry.enabled": false,
    "markdown.extension.toc.slugifyMode": "azureDevops",
    "markdown.extension.completion.enabled": true,
    "cSpell.enableFiletypes": [
        "al"
    ],
    "window.zoomLevel": -1,
    "git.openRepositoryInParentFolders": "always",
    "editor.stickyScroll.enabled": false,
    "editor.minimap.enabled": false,
    "editor.rulers": [
        120
    ],
    "[git-commit]": {
        "editor.rulers": [
            50,
            72
        ],
        "editor.wordWrap": "off"
    },
    "github.copilot.enable": {
        "*": false,
        "markdown": true
    },
    "markdown.updateLinksOnFileMove.enabled": "prompt",
    "markdown.validate.enabled": true,
    "markdown.extension.print.absoluteImgPath": false,
    "markdown.extension.print.theme": "dark",
    "terminal.integrated.fontFamily": "'Cousine Nerd Font Mono'",
    "vsicons.dontShowNewVersionMessage": true,
    "yaml.schemas": {
        "https://squidfunk.github.io/mkdocs-material/schema.json": "mkdocs.yml"
    },
    "yaml.customTags": [ 
        "!ENV scalar",
        "!ENV sequence",
        "!relative scalar",
        "tag:yaml.org,2002:python/name:material.extensions.emoji.to_svg",
        "tag:yaml.org,2002:python/name:material.extensions.emoji.twemoji",
        "tag:yaml.org,2002:python/name:pymdownx.superfences.fence_code_format"
    ],
    "github.copilot.editor.enableCodeActions": false,
    "workbench.startupEditor": "none",
    "files.dialog.defaultPath": "C:\\Users\\SaraThorhallsdottir\\Documents",
    "files.simpleDialog.enable": true,
    "workbench.colorCustomizations": {
        "[Lunar Pink Satellite]": {"sideBar.background": "#202020"}
    },
    "editor.tokenColorCustomizations": {
        "[Lunar Pink Satellite]": {
            "functions": "#fca17d",
            "variables": "#c97eb5"
        }
    },
    "editor.foldingImportsByDefault": true,
    "explorer.compactFolders": false,
    "github.copilot.editor.enableAutoCompletions": false,
    "github.copilot.chat.copilotDebugCommand.enabled": false,
    "workbench.sideBar.location": "right",
    "workbench.editor.empty.hint": "hidden"
}
